<p id="notice"><%= notice %></p>

<h1>Listing Kind Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kind_transactions.each do |kind_transaction| %>
      <tr>
        <td><%= kind_transaction.description %></td>
        <td><%= link_to 'Show', kind_transaction %></td>
        <td><%= link_to 'Edit', edit_kind_transaction_path(kind_transaction) %></td>
        <td><%= link_to 'Destroy', kind_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kind transaction', new_kind_transaction_path %>
